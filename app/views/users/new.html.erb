<h1>Users#new</h1>

<%= form_for @user do |f| %>
	<%= f.label :email, "Email:" %>
	<%= f.email_field :email %><br/><br/>

	<%= f.label :username, "Username:" %>
	<%= f.text_field :username %><br/><br/>

	<%= f.label :password, "Password:" %>
	<%= f.password_field :password %><br/><br/>

	<%= f.submit %>
<% end %>
